{
    "_description": "DistilBERT test configuration (small dataset, fast iterations)",
    
    "model_type": "distilbert",
    
    "model_config": {
        "hidden_size": 768,
        "num_attention_heads": 12,
        "num_hidden_layers": 6,
        "max_seq_length": 128,
        "vocab_size": 10000,
        "dropout": 0.1
    },
    
    "observation_space": {
        "max_num_stores_loads": 7,
        "max_num_loops": 12,
        "max_num_load_store_dim": 12,
        "num_tile_sizes": 7,
        "vect_size_limit": 512
    },
    
    "action_space": {
        "order": [["I"], ["!", "I", "NT"], ["!", "I"], ["V", "NT"]],
        "interchange_mode": "pointers"
    },
    
    "exploration": {
        "strategy": ["entropy"],
        "init_epsilon": 0.5
    },
    
    "architecture": {
        "new_architecture": false,
        "activation": "relu",
        "normalize_bounds": "max",
        "normalize_adv": "standard"
    },
    
    "reward": {
        "sparse_reward": true,
        "split_ops": true
    },
    
    "training": {
        "bench_count": 8,
        "replay_count": 5,
        "nb_iterations": 3,
        "reuse_experience": "none",
        "truncate": 5
    },
    
    "ppo": {
        "ppo_epochs": 4,
        "ppo_batch_size": 16,
        "lr": 0.0001,
        "gamma": 0.99,
        "clip_epsilon": 0.2,
        "entropy_coef": 0.01
    },
    
    "value_function": {
        "value_epochs": 0,
        "value_batch_size": 16,
        "value_coef": 0.5,
        "value_clip": false
    },
    
    "data_paths": {
        "benchmarks_folder_path": "data/test/code_files",
        "json_file": "data/test/execution_times_train.json",
        "eval_json_file": "data/test/execution_times_eval.json"
    },
    
    "logging": {
        "tags": ["distilbert", "test", "quick"],
        "debug": false,
        "results_dir": "results",
        "main_exec_data_file": ""
    }
}
